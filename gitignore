# Ignore operating system files
.DS_Store
Thumbs.db

# Ignore IDE and editor configuration files
.idea/
.vscode/
*.swp

# Ignore dependency directories
node_modules/
vendor/
__pycache__/

# Ignore build and output directories
dist/
build/
out/
target/

# Ignore log files
*.log

# Ignore environment and secret files
.env
.env.local
*.key

# Ignore temporary files and backups
*.tmp
*.bak
*.old

# Ignore compiled files
*.class
*.o
*.pyc
*.pyo

# Ignore coverage reports
coverage/
*.lcov

# Ignore package manager lock files (optional)
package-lock.json
yarn.lock

# Ignore R-specific files (if using R)
.Rhistory
.RData
.Rproj.user/

# Ignore Python virtual environments (if using Python)
.venv/
